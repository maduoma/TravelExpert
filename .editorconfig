# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false



#indent_style
#indent_size
#tab_width
#end_of_line (on save)
#insert_final_newline (on save)
#trim_trailing_whitespace (on save)
